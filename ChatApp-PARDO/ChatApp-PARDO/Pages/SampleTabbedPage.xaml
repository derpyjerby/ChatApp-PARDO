<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ChatApp_PARDO.SampleTabbedPage"
             xmlns:local="clr-namespace:ChatApp_PARDO"
             xmlns:android="clr-namespace:Xamarin.Forms.PlatformConfiguration.AndroidSpecific;assembly=Xamarin.Forms.Core"
             android:TabbedPage.ToolbarPlacement="Bottom"
             android:TabbedPage.BarItemColor="DarkGray"
             android:TabbedPage.BarSelectedItemColor="White"
             BarBackgroundColor="#1976D2">
    
    <local:ChatPage Title="Chat" Icon="ChatIcon"/>
    <local:ProfilePage x:Name="profilePage" Title="Profile" Icon="PersonIcon"/>
</TabbedPage>